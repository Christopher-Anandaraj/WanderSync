<?xml version="1.0" encoding="utf-8"?>
    <!-- Start of Allyson Implementation -->

    <!-- Linear layout for dining entries -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"

    android:padding="16dp">

    <!-- Text for location in dining entry -->
    <TextView
        android:id="@+id/reservation_entry_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <!-- Text for restaurant in dining entry -->
    <TextView
        android:id="@+id/reservation_entry_restaurant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="McDonalds"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <!-- Text for time in dining entry -->
    <TextView
        android:id="@+id/reservation_entry_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <!-- Text for link in dining entry -->
    <TextView
        android:id="@+id/reservation_entry_link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="someplace.com"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

</LinearLayout>

    <!-- End of Allyson Implementation -->