<?xml version="1.0" encoding="utf-8"?>
<!-- Start of Allyson Implementation -->

<!-- Linear layout for dining entries -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="16dp">

    <!-- Text for location in community entry -->
    <TextView
        android:id="@+id/community_entry_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <!-- Text for dining entry in community -->
    <TextView
        android:id="@+id/community_entry_dining"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Food Review"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <!-- Text for accommodations entry in community -->
    <TextView
        android:id="@+id/community_entry_accomodations"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/community_duration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="XX/XX/XXXX"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

</LinearLayout>

    <!-- End of Allyson Implementation -->